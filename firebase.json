{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": [
    {
      "public": "dist/freedom",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "freedom",
      "source": ".",
      "frameworksBackend": {}
    }
  ],
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  },
  "extensions": {}
}